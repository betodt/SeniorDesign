<div id= "projectView">
	<h2 class="center-align">YOUR PROJECTS</h2>
	{{outlet}}
	<div class="container center-align">
		<div class="row">
			{{#each model as |user|}}
				<div class="col s3 project"><h5 class="center-align">{{link-to user.firstName "projects.tasks" user}}</h5></div>
			{{/each}}
		</div>
	</div>
		<a class="btn-floating btn-small waves-effect waves-light grey" {{action "openProjectModel"}}><i class="material-icons">add</i></a>
</div>

